<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Rsystems BEP UI Testing">

	<listeners>
		<listener class-name="com.rsystems.utils.TestListenerAdapter"></listener>
	</listeners>

	<test name="Login Module Test case ">
		<classes>
			<class name="com.rsystems.test.LogInPageTestCase">
				<methods>
					<include name="tc_BEP290001_LoginWithValidCredentials" />
					<include name="tc_BEP290002_EncryptedPasswordValidation" />
					<include name="tc_BEP290003_Login_ValidUserAndInValidPassword" />
					<include name="tc_BEP290004_Login_InValidUserAndValidPassword" />
					<include name="tc_BEP290005_Login_InvalidUserAndInvalidPassword" />
					<include name="tc_BEP290006_ValidationWithEmptyUserAndEmptyPassword" />
					<include
						name="tc_BEP290007_LoginWithUsernamePasswordLength_1to30_and_1to20" />
					<include name="tc_BEP290008_VerifyLengthOfUserNameAndPassword" />
					<include name="tc_BEP290009_VerifyKeyboardNavigationKeys" />
					<include name="tc_BEP290010_VerifyCompanyLogo" />
					<include name="tc_BEP290011_VerifyDefaultTextUsernameAndPassword" />
					<include name="tc_BEP290012_VerifyLogoMessage" />
				</methods>
			</class>
		</classes>
	</test>
</suite>

